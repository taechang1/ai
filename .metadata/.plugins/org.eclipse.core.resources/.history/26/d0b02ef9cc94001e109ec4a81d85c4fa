	String 이름;
	String 학과;
	int 점수;
	String 전화번호;
	
	
	public Student(String 학번, String 이름, String 학과, int 점수, String 전화번호) {
		// TODO Auto-generated constructor stub
		this.학번=학번;
		this.이름=이름;
		this.학과=학과;
		this.점수=점수;
		this.전화번호=전화번호;
	}

	//점수 계산하기
	int 점수계산() {
		return 0;
	}
	void 성적출력하기() {
		
	}
	
    void 입력하기(){
		
	}
    String 장학금대상자() {
    	return "홍길동";
    }
    
    boolean 합격여부구하기(){
    	return true;
    }

}

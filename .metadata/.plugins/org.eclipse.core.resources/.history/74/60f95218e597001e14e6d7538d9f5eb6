package javaBasic3.ch06.seco14;

import java.util.Scanner;

public class BankApplication {

    Scanner sc=new Scanner(System.in);	
	
	Account[] accounts=new Account[100];
	int count=0;

	public void 썯촥쨬() {
		accounts[count]=new Account();
		System.out.println("-----");
		System.out.println("썯촥 쨬");
		System.out.println("-----");
		System.out.println("썯촥 좾탺");
		accounts[count].썯촥좾탺=sc.next();
		System.out.println("썯촥촸");
		accounts[count].썯촥촸=sc.next();
		System.out.println("퉘舅쐏쮕:");
		accounts[count].퉘舅쐏쮕=sc.nextInt();
		System.out.println("써쎀:썯촥쌰 쨬왆쮴챲핎.");
	    count=count+1;
		
	
	}
    //썯촥 쨬 
	//썯촥 점
	//찣쐏
	//歐
	
	public void 썯촥점()
	

package javaBasic.ch03.sec02;

public class Operator01 {

	public static void main(String[] args) {
		int number=10;//엄청 많이 쓰는 수식
		number++;
		//number=number+1;//엄청 많이 쓰느 수식
		
		System.out.println(number);
		
		//number=number-1;
		number--;
		
		System.out.println(number);
		
		int number1=100;
		//101이 나오게 하기
		number1++;
		
		System.out.println(number1);
		
		int number2=number1;
		//101이 나오게 하기
		
		System.out.println(number2);
		
		int number3=1000;
		
		number3++;
		
		System.out.println(number3);
        int number4=number3;
		
		number3++;
		
		System.out.println(number4);
		
		int number5=20;
		int number6=number5++;//1번 6번 =5 2번 5번은 5+1 21
		
		System.out.println(number5);
		System.out.println(number6);
		
		int number7=200;
		int number8=++number7;//1,번 7번 7+1-- 2번 8번 =7번
		
		System.out.println(number7);
		System.out.println(number8);
		
		int nu9=2000;
		System.out.println(nu9++);
		System.out.println(nu9);
		
		int nu10=2000;
		System.out.println(nu10);
		System.out.println(++nu10);

		
	}

}

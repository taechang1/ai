package javaBasic2.ch5.sec01;

public class 참조타입 {

	public static void main(String[] args) {
		
		
		int n=10; // 스택공간
		
		Scanner sc = new Scanner(System.in); //new 힙 메모리에 Scanner크래스를 올려주세요.
		// 힙메모리에 주소를 스택 공간의 변수에 저장해야한다.
		
		System.out.println(n);
		st=new Student();
// 코드 작성 -설계도 사용하려면 메모리에 올려 놓고 자료(new) 사용하기 시멘트 철근 힙 메모리에 올라감
	}

}

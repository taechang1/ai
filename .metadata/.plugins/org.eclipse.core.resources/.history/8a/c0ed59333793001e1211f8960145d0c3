package javaBasic.ch03.sec02;

public class if문 {

	public static void main(String[] args) {
		// 만약에 조건이 참이면 거짓이면
		//90~100"A" 89~90 "B"
		int score=87;
		
		if(score>=90) {
			System.out.println("A");
		}
		if(80<=score&&score<=89) {
		System.out.println("b");
		}
	int a=20,b=10,c=20;
	
	if(a >=10&& b <=20 || c ==20) {
		System.out.println("합격");
	}
	}
}
   // 변수

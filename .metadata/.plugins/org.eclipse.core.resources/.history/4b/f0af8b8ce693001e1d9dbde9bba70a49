package javaBasic.ch04.sec02;

public class 확인문제4장 {

	public static void main(String[] args) {
		// 3
		//for문1~100까지 나오게 하기
		int sum3=0;
		for(int i=1;i<=100;i++) {
			if(i%3==0) {
				sum3=sum3 +i;
			}
		}
		System.out.println("1~100까지 3의 배수 합은"+sum3);
		//5
		for(int x=1;x<=10;x++) {
			for(int y=1;y<10;y++) {
				if(4*x+5*y==60) {
					System.out.println("x="+x+" y="+y);
			}
		}
	}
//6
		for(int i=1; i<=5;i++) {
			System.out.print("*");
			
		}
		System.out.println();
    }
}
